<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <Button
        android:id="@+id/common_test_upload_fail_dialog_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="弹出uploadFailDialog"/>
    <Button
        android:id="@+id/common_test_upload_progress_dialog_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="弹出UploadProgressDialog"/>
    <Button
        android:id="@+id/common_test_time_dialog_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="弹出时间选择对话框"/>
    <Button
        android:id="@+id/common_test_date_dialog_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="弹出日期选择对话框"/>
</LinearLayout>
